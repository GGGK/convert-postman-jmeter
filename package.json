{
  "name": "convert-postman-jmeter",
  "version": "1.0.6-beta",
  "description": "Convert files Postman Project to JMeter Project",
  "main": "./lib/convert-postman-jmeter",
  "scripts": {
    "start": "node ./bin/convert-postman-jmeter",
    "test": "mocha test --reporter mocha-junit-reporter --reporter-options mochaFile=./reports/mocha/test-results.xml && nyc --reporter=lcov --reporter=text-lcov mocha test/*.js",
    "lint": "eslint ./lib --format junit --output-file ./reports/eslint/eslint.xml && eslint ./lib --format json -o ./reports/eslint/eslint.json"
  },
  "files": [
    "lib",
    "bin",
    "templates"
  ],
  "keywords": [
    "converter",
    "jmeter",
    "postman",
    "cli",
    "command"
  ],
  "author": "chancaysergio@gmail.com",
  "license": "MIT",
  "preferGlobal": true,
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "homepage": "https://github.com/sercheo87/convert-postman-jmeter#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/sercheo87/convert-postman-jmeter"
  },
  "bugs": {
    "url": "https://github.com/sercheo87/convert-postman-jmeter/issues"
  },
  "bin": {
    "convert-postman-jmeter": "./bin/convert-postman-jmeter"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.13.0",
    "mocha": "^6.1.4",
    "mocha-junit-reporter": "^1.22.0",
    "mocha-sonar-generic-test-coverage": "0.0.6",
    "nyc": "^14.1.1"
  },
  "dependencies": {
    "mustache": "^3.0.1",
    "yargs": "^13.2.4"
  }
}
